{
  "name": "clevabit-sdk-nodejs",
  "version": "1.0.1",
  "description": "clevabit SDK for node.js",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "prepublish": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "build": "tsc",
    "test": "jest --config jestconfig.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json"
  },
  "files": [
    "lib/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/clevabit/clevabit-sdk-nodejs.git"
  },
  "author": "clevabit GmbH",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/clevabit/clevabit-sdk-nodejs/issues"
  },
  "homepage": "https://github.com/clevabit/clevabit-sdk-nodejs#readme",
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@types/jws": "^3.2.2",
    "@types/mqtt": "^2.5.0",
    "@types/ms": "^0.7.31",
    "@types/node": "^14.14.6",
    "@types/node-fetch": "^2.5.7",
    "jest": "^26.6.1",
    "prettier": "^2.1.2",
    "ts-jest": "^26.4.3",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.0.5"
  },
  "dependencies": {
    "jws": "^4.0.0",
    "mqtt": "^4.2.4",
    "ms": "^2.1.2",
    "node-fetch": "^2.6.1"
  }
}
